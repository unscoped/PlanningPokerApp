  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.3.2",
    "@types/supertest": "^2.0.11",
    "@types/ws": "^7.2.3",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "pure-uuid": "^1.6.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "ws": "^7.5.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.12",
    "supertest": "^6.1.3",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2"
  }
